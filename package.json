{
  "name": "d-ui",
  "version": "1.0.0",
  "description": "an ui lib for practice",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": [
    "dist/*",
    "esm/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "init": "f2elint init",
    "dev": "dumi dev",
    "clean": "rimraf lib esm dist",
    "build:lib": "gulp",
    "build:site": "rimraf doc-site && dumi build",
    "preview:site": "npm run build:site && serve doc-site",
    "build:types": "tsc -p tsconfig.build.json && cpr lib esm",
    "publish:patch": "standard-version --release-as patch && git push --follow-tags && npm publish",
    "publish:minor": "standard-version --release-as minor && git push --follow-tags && npm publish",
    "publish:major": "standard-version --release-as major && git push --follow-tags && npm publish",
    "publish:prerelease": "standard-version --release-as patch --prerelease --skip.tag=true && git push && npm publish",
    "patch": "run-s clean build:* publish:patch",
    "minor": "run-s clean build:* publish:minor",
    "major": "run-s clean build:* publish:major",
    "prerelease": "run-s clean build:* publish:prerelease",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix"
  },
  "author": "d+",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/testing-library__react": "^10.2.0",
    "cpr": "^3.0.1",
    "dumi": "^1.1.40",
    "f2elint": "^2.2.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jsdom": "^16.7.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "standard-version": "^9.5.0",
    "through2": "^4.0.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@babel/runtime-corejs3": "^7.17.9"
  }
}
